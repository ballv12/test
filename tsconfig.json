{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "ESNext",
	  "lib": ["DOM", "ES2020"],
	  "moduleResolution": "Bundler",
	  "allowJs": true,
	  "checkJs": false,
	  "resolveJsonModule": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "skipLibCheck": true,
	  "strict": true,
	  "types": ["svelte", "node"]
	},
	"include": ["src/**/*", "vite.config.ts"],
	"exclude": ["node_modules", ".svelte-kit", "build"]
  }
  